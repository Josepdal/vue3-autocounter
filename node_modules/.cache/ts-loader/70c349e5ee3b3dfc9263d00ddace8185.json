CAA9B","sourcesContent":["\nimport { defineComponent } from \"vue\";\n\ninterface Data {\n\tcurrentAmount: number;\n}\n\nexport default defineComponent({\n\tname: \"Vue3Autocounter\",\n  props: {\n    startAmount: {\n      type: Number,\n      default: 0\n    },\n    endAmount: {\n      type: Number,\n      default: 0,\n      required: true\n    },\n    duration: {\n      type: Number,\n      default: 1000\n    },\n    autoplay: {\n      type: Boolean,\n      default: true\n    },\n    prefix: {\n      type: String,\n      default: ''\n    },\n    suffix: {\n      type: String,\n      default: ''\n    },\n    decimals: {\n      type: Number,\n      default: 0,\n      validator(decimals: number): boolean {\n        return decimals >= 0;\n      },\n    },\n    decimal: {\n      type: String,\n      default: '.'\n    },\n    separator: {\n      type: String,\n      default: ','\n    }\n  },\n\tdata(): Data {\n\t\treturn {\n\t\t\tcurrentAmount: 0\n\t\t};\n\t},\n  created (): void {\n    this.counting();\n  },\n  computed: {\n    isCountingUp(): boolean {\n      return this.endAmount >= this.startAmount;\n    },\n    amountCounted(): number {\n      if (this.isCountingUp) return this.endAmount - this.startAmount;\n      else return this.startAmount - this.endAmount;\n    },\n    incrementer(): number {\n      return this.amountCounted / (this.duration / 10);\n    },\n    displayedAmount(): string {\n      return `${this.prefix}${this.formatedAmount}${this.suffix}`;\n    },\n    formatedAmount(): string {\n      let num = this.currentAmount.toFixed(this.decimals);\n      num += '';\n      const x = num.split('.');\n      let x1 = x[0];\n      const x2 = x.length > 1 ? this.decimal + x[1] : '';\n      const rgx = /(\\d+)(\\d{3})/;\n      if (this.separator) {\n        while (rgx.test(x1)) {\n          x1 = x1.replace(rgx, '$1' + this.separator + '$2');\n        }\n      }\n      return x1 + x2;\n    }\n  },\n\tmethods: {\n    counting(): void {\n\t\t\tif (\n        (this.isCountingUp && this.currentAmount < this.endAmount)\n        ||Â (!this.isCountingUp && this.currentAmount > this.endAmount)\n      ) {\n\t\t\t\tsetTimeout(() => {\n\t\t\t\t\tif (this.isCountingUp) this.currentAmount += this.incrementer;\n\t\t\t\t\telse this.currentAmount -= this.incrementer;\n\t\t\t\t\tthis.counting();\n\t\t\t\t}, 10);\n\t\t\t}\n\t\t}\n\t},\n});\n"],"sourceRoot":""}]}